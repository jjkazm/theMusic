<% include ./partials/header %>

<div class="container">
    <div class="row">
        <div class="col-md-6 col-sm-12">
            <h1>Sign Up!</h1>
            <form action="/register" method="post">
                <input type="text" name="newUser[username]" placeholder="username">
                <input type="text" name="newUser[email]" placeholder="email">
                <input type="password" name="newUser[password]" placeholder="password">
                <input type="text" name="newUser[country]" placeholder="country">
                <input type="text" name="newUser[age]" placeholder="age">
                <button>Sign Up</button>
            </form>
        </div>
    </div>
</div>


<% include ./partials/footer %>